# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# SQS Configuration
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account-id/job-queue
SQS_DLQ_URL=https://sqs.us-east-1.amazonaws.com/your-account-id/job-queue-dlq

# PostgreSQL Configuration
